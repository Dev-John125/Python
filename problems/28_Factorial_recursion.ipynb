    "## 28. To find the factorial of a number using recursion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "9bf97a01",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a number: 10\n",
      "The factorial is : 3628800\n"
     ]
    }
   ],
   "source": [
    "def fact(n):\n",
    "    if (n>=1):\n",
    "        return n*fact(n-1)\n",
    "    else:\n",
    "        return 1;\n",
    "\n",
    "a=int(input(\"Enter a number: \"))\n",
    "print(\"The factorial is :\",fact(a))"
   ]
  },
