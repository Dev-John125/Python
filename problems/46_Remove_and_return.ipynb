   "source": [
    "## 46. To remove the item at the given index from the list and return it"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "19e6b749",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the index position: 4\n",
      "The removed element is 2\n"
     ]
    }
   ],
   "source": [
    "def remove(li,index):\n",
    "    key=li[index]\n",
    "    li.remove(index)\n",
    "    return key\n",
    "\n",
    "li=[9,1,4,6,2,7,8]\n",
    "i=int(input(\"Enter the index position: \"))\n",
    "print(\"The removed element is\",remove(li,i))"
   ]
  },
